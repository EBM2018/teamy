{
  "name": "Teamy-Backend",
  "version": "1.0.0",
  "description": "Backend du Gestion des groupes, EBM 18-19",
  "main": "index.js",
  "scripts": {
    "start": "dotenv nodemon src/index.js",
    "prod": "dotenv node src/index.js",
    "setup-env": "node -e \"(fs => fs.existsSync('.env') || fs.copyFileSync('.env.example', '.env'))(require('fs'))\"",
    "test": "eslint src/",
    "apidoc": "apidoc -i src/ -o doc/"
  },
  "keywords": [
    "express",
    "boilerplate"
  ],
  "apidoc": {
    "title": "ApiDoc pour le projet Teamy",
    "url": "/api",
    "sampleUrl": "http://localhost:4000/api"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "docker": "^1.0.0",
    "dotenv-cli": "^1.4.0",
    "express": "^4.16.4",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.8",
    "request": "^2.88.0",
    "serve-static": "^1.13.2"
  },
  "devDependencies": {
    "apidoc": "^0.17.7",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "nodemon": "^1.18.9"
